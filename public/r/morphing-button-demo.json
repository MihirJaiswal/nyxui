    {
      "path": "registry/example/morphing-button-demo.tsx",
      "content": "import React from \"react\";\nimport { MorphingButton } from \"../ui/morphing-button\";\nimport {\n  ActivitySquareIcon,\n  ArrowRight,\n  Droplet,\n  Heart,\n  Plus,\n} from \"lucide-react\";\n\nexport const MorphingButtonDemo = () => {\n  return (\n    <div className=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-4 w-full\">\n      <div className=\"flex justify-center items-center h-48 border border-gray-300 dark:border-gray-800 rounded-lg\">\n        <MorphingButton\n          variant=\"expand\"\n          icon={<ArrowRight />}\n          iconPosition=\"right\"\n          className=\"bg-gradient-to-r from-blue-500 to-indigo-600 text-white border-0 hover:shadow-lg rounded-lg\"\n        >\n          Expand\n        </MorphingButton>\n      </div>\n\n      <div className=\"flex justify-center items-center h-48 border border-gray-300 dark:border-gray-800 rounded-lg\">\n        <MorphingButton\n          variant=\"collapse\"\n          icon={<ArrowRight />}\n          iconPosition=\"left\"\n          className=\"bg-gradient-to-r from-emerald-500 to-teal-600 text-white border-0 rounded-lg\"\n        >\n          Collapse\n        </MorphingButton>\n      </div>\n\n      <div className=\"flex justify-center items-center h-48 border border-gray-300 dark:border-gray-800 rounded-lg\">\n        <MorphingButton\n          variant=\"rotate\"\n          icon={<Plus />}\n          iconPosition=\"left\"\n          className=\"bg-gradient-to-r from-amber-400 to-orange-500 text-white border-0 rounded-lg\"\n        >\n          Rotate\n        </MorphingButton>\n      </div>\n\n      <div className=\"flex justify-center items-center h-48 border border-gray-300 dark:border-gray-800 rounded-lg\">\n        <MorphingButton\n          variant=\"pulse\"\n          iconPosition=\"only\"\n          className=\"bg-gradient-to-r from-red-500 to-rose-600 text-white border-0 rounded-full w-12\"\n        >\n          <Heart />\n        </MorphingButton>\n      </div>\n\n      <div className=\"flex justify-center items-center h-48 border border-gray-300 dark:border-gray-800 rounded-lg\">\n        <MorphingButton\n          variant=\"liquid\"\n          size=\"lg\"\n          icon={<Droplet />}\n          iconPosition=\"right\"\n          className=\"bg-gradient-to-r from-violet-500 to-purple-600 text-white border-0 shadow-md\"\n        >\n          Liquid\n        </MorphingButton>\n      </div>\n\n      <div className=\"flex justify-center items-center h-48 border border-gray-300 dark:border-gray-800 rounded-lg\">\n        <MorphingButton\n          variant=\"bounce\"\n          icon={<ActivitySquareIcon />}\n          iconPosition=\"right\"\n          className=\"bg-gradient-to-r from-lime-400 to-green-500 text-white border-0 rounded-lg\"\n        >\n          Bounce\n        </MorphingButton>\n      </div>\n    </div>\n  );\n};\n",
      "type": "registry:example",
      "target": "components/morphing-button-demo.tsx"
    }
  ]
}