      "content": "import React from \"react\";\r\nimport { Button } from \"../../components/ui/button\";\r\nimport { ArrowRight, Star, Users, Zap } from \"lucide-react\";\r\n\r\nexport function CTASection() {\r\n  return (\r\n    <section className=\"relative py-24 bg-gradient-to-br from-blue-600 via-purple-600 to-indigo-700 overflow-hidden\">\r\n      {/* Background decoration */}\r\n      <div className=\"absolute inset-0 bg-black/10\" />\r\n      <div className=\"absolute top-0 left-0 w-full h-full\">\r\n        <div className=\"absolute top-20 left-20 w-96 h-96 bg-white/10 rounded-full blur-3xl\"></div>\r\n        <div className=\"absolute bottom-20 right-20 w-96 h-96 bg-white/10 rounded-full blur-3xl\"></div>\r\n      </div>\r\n\r\n      <div className=\"relative z-10 container mx-auto px-4 sm:px-6 lg:px-8\">\r\n        <div className=\"text-center max-w-4xl mx-auto\">\r\n          {/* Badge */}\r\n          <div className=\"inline-flex items-center rounded-full bg-white/20 backdrop-blur-sm border border-white/30 px-4 py-2 text-sm font-medium text-white mb-8\">\r\n            <Star className=\"h-4 w-4 mr-2 fill-yellow-400 text-yellow-400\" />\r\n            Join 10,000+ developers\r\n          </div>\r\n\r\n          {/* Main heading */}\r\n          <h2 className=\"text-4xl sm:text-5xl lg:text-6xl font-bold text-white mb-6\">\r\n            Ready to build something\r\n            <span className=\"block bg-gradient-to-r from-yellow-300 to-orange-300 bg-clip-text text-transparent\">\r\n              amazing?\r\n            </span>\r\n          </h2>\r\n\r\n          {/* Subtitle */}\r\n          <p className=\"text-xl text-blue-100 mb-12 max-w-2xl mx-auto leading-relaxed\">\r\n            Start building beautiful, accessible, and performant user interfaces today. \r\n            Join thousands of developers who trust Nyx UI for their projects.\r\n          </p>\r\n\r\n          {/* CTA buttons */}\r\n          <div className=\"flex flex-col sm:flex-row gap-4 justify-center items-center mb-16\">\r\n            <Button size=\"lg\" className=\"bg-white text-blue-600 hover:bg-gray-100 group text-lg px-8 py-4\">\r\n              Get Started Free\r\n              <ArrowRight className=\"ml-2 h-5 w-5 group-hover:translate-x-1 transition-transform\" />\r\n            </Button>\r\n            <Button variant=\"outline\" size=\"lg\" className=\"border-white text-white hover:bg-white/10 group text-lg px-8 py-4\">\r\n              View Documentation\r\n            </Button>\r\n          </div>\r\n\r\n          {/* Stats */}\r\n          <div className=\"grid grid-cols-1 sm:grid-cols-3 gap-8 max-w-3xl mx-auto\">\r\n            <div className=\"text-center\">\r\n              <div className=\"flex items-center justify-center mb-2\">\r\n                <Users className=\"h-8 w-8 text-blue-200 mr-2\" />\r\n                <div className=\"text-3xl font-bold text-white\">10K+</div>\r\n              </div>\r\n              <div className=\"text-blue-200\">Active Developers</div>\r\n            </div>\r\n            <div className=\"text-center\">\r\n              <div className=\"flex items-center justify-center mb-2\">\r\n                <Zap className=\"h-8 w-8 text-blue-200 mr-2\" />\r\n                <div className=\"text-3xl font-bold text-white\">50+</div>\r\n              </div>\r\n              <div className=\"text-blue-200\">Components</div>\r\n            </div>\r\n            <div className=\"text-center\">\r\n              <div className=\"flex items-center justify-center mb-2\">\r\n                <Star className=\"h-8 w-8 text-blue-200 mr-2\" />\r\n                <div className=\"text-3xl font-bold text-white\">4.9</div>\r\n              </div>\r\n              <div className=\"text-blue-200\">Star Rating</div>\r\n            </div>\r\n          </div>\r\n\r\n          {/* Trust indicators */}\r\n          <div className=\"mt-16 pt-8 border-t border-white/20\">\r\n            <p className=\"text-blue-200 mb-6\">Trusted by teams at</p>\r\n            <div className=\"flex flex-wrap justify-center items-center gap-8 opacity-60\">\r\n              <div className=\"text-white font-semibold text-lg\">Microsoft</div>\r\n              <div className=\"text-white font-semibold text-lg\">Google</div>\r\n              <div className=\"text-white font-semibold text-lg\">Netflix</div>\r\n              <div className=\"text-white font-semibold text-lg\">Stripe</div>\r\n              <div className=\"text-white font-semibold text-lg\">Vercel</div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n  );\r\n}\r\n",
