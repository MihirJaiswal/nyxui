      "content": "import React from \"react\";\nimport { LiquidMetalButton } from \"../ui/liquid-metal-button\";\nimport {\n  Crown,\n  Download,\n  Shield,\n  ShoppingCart,\n  Trophy,\n  Zap,\n} from \"lucide-react\";\n\nexport const LiquidMetalButtonDemo = () => {\n  return (\n    <div className=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-4 w-full relative\">\n      <div className=\"flex justify-center items-center h-48 border border-gray-300 dark:border-gray-800 rounded-lg\">\n        <LiquidMetalButton\n          variant=\"default\"\n          theme=\"gold\"\n          className=\"rounded-lg\"\n          size=\"lg\"\n        >\n          <div className=\"flex items-center\">\n            <Crown className=\"mr-2 h-4 w-4\" />\n            <span>Gold</span>\n          </div>\n        </LiquidMetalButton>\n      </div>\n\n      <div className=\"flex justify-center items-center h-48 border border-gray-300 dark:border-gray-800 rounded-lg\">\n        <LiquidMetalButton\n          variant=\"default\"\n          theme=\"silver\"\n          className=\"rounded-lg\"\n        >\n          <div className=\"flex items-center\">\n            <Shield className=\"mr-2 h-4 w-4\" />\n            <span>Silver</span>\n          </div>\n        </LiquidMetalButton>\n      </div>\n\n      <div className=\"flex justify-center items-center h-48 border border-gray-300 dark:border-gray-800 rounded-lg\">\n        <LiquidMetalButton\n          variant=\"default\"\n          theme=\"copper\"\n          className=\"rounded-lg\"\n        >\n          <div className=\"flex items-center\">\n            <Trophy className=\"mr-2 h-4 w-4\" />\n            <span>Copper</span>\n          </div>\n        </LiquidMetalButton>\n      </div>\n      <div className=\"flex justify-center items-center h-48 border border-gray-300 dark:border-gray-800 rounded-lg\">\n        <LiquidMetalButton\n          variant=\"default\"\n          theme=\"steel\"\n          className=\"rounded-lg\"\n        >\n          <div className=\"flex items-center\">\n            <Zap className=\"mr-2 h-4 w-4\" />\n            <span>Steel</span>\n          </div>\n        </LiquidMetalButton>\n      </div>\n\n      <div className=\"flex justify-center items-center h-48 border border-gray-300 dark:border-gray-800 rounded-lg\">\n        <LiquidMetalButton\n          variant=\"outline\"\n          theme=\"gold\"\n          className=\"rounded-lg\"\n        >\n          <div className=\"flex items-center\">\n            <Crown className=\"mr-2 h-4 w-4\" />\n            <span>Outline</span>\n          </div>\n        </LiquidMetalButton>\n      </div>\n\n      <div className=\"flex justify-center items-center h-48 border border-gray-300 dark:border-gray-800 rounded-lg\">\n        <LiquidMetalButton\n          variant=\"mercury\"\n          theme=\"mercury\"\n          intensity={5}\n          className=\"rounded-lg\"\n        >\n          <div className=\"flex items-center\">\n            <Download className=\"mr-2 h-4 w-4\" />\n            <span>Mercury Flow</span>\n          </div>\n        </LiquidMetalButton>\n      </div>\n\n      <div className=\"flex justify-center items-center h-48 border border-gray-300 dark:border-gray-800 rounded-lg\">\n        <LiquidMetalButton\n          variant=\"ripple\"\n          theme=\"steel\"\n          intensity={4}\n          className=\"rounded-lg\"\n        >\n          <div className=\"flex items-center\">\n            <ShoppingCart className=\"mr-2 h-4 w-4\" />\n            <span>Ripple Wave</span>\n          </div>\n        </LiquidMetalButton>\n      </div>\n\n      <div className=\"flex justify-center items-center h-48 border border-gray-300 dark:border-gray-800 rounded-lg\">\n        <LiquidMetalButton\n          variant=\"mercury\"\n          theme=\"gold\"\n          intensity={5}\n          textured={true}\n          className=\"rounded-lg relative\"\n        >\n          <div className=\"flex items-center\">\n            <Crown className=\"mr-2 h-4 w-4\" />\n            <span>Gold Premium</span>\n          </div>\n        </LiquidMetalButton>\n      </div>\n\n      <div className=\"flex justify-center items-center h-48 border border-gray-300 dark:border-gray-800 rounded-lg\">\n        <LiquidMetalButton\n          variant=\"ripple\"\n          theme=\"steel\"\n          intensity={5}\n          textured={true}\n          className=\"rounded-lg relative\"\n        >\n          <div className=\"flex items-center\">\n            <Zap className=\"mr-2 h-4 w-4\" />\n            <span>Steel Elite</span>\n          </div>\n        </LiquidMetalButton>\n      </div>\n    </div>\n  );\n};\n",
      "type": "registry:example",
