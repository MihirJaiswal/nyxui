    "typecheck": "pnpm build:docs && tsc --noEmit",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:fix": "prettier --write \"**/*.{ts,tsx,mdx}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\"",
    "build:docs": "content-collections build",
